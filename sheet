6-

x=int(input('enter the first number : '))
y=int(input('enter the first number : '))
count=1
var=0
if x > y:
    while count < x:
        if x%count==0:
            if y%count==0:
                var=count
        count=count+1
else:    
    while count < y:
        if x%count==0:
            if y%count==0:
                var=count
        count=count+1
print('gcd =',var) 
--------------------------------------------------------------------------------------------------------
7-

def letter(var):
    m=len(var)
    print('total letters =',m)
    v=1/m
    dict = {}
    for i in var:
        if i in dict:
            dict[i] += v
        else:
            dict[i] = v
    
    for k, v in dict.items():
          print(k,'=', v)
                            
var=str(input('enter string')) 
letter(var)



